		"@vitest/ui": "^4.0.7",
		"happy-dom": "^20.0.10",
		"jsdom": "^27.1.0",
		"svelte": "^5.41.0",
		"svelte-check": "^4.3.3",
		"tailwindcss": "^4.1.17",
